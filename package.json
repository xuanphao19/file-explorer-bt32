    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "vite": "^7.0.4"
  },
  "dependencies": {
    "axios": "^1.10.0"
  }
}
